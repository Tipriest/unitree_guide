<launch>
    <node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="lidar_pointcloud_to_laserscan" 
        respawn="false" output="screen">
        <remap from="cloud_in" to="/vlp16_points"/>
        <remap from="/scan" to="/lidarLaserScan"/>
        <rosparam>
            target_frame: VLP-16
            transform_tolerance: 0.01
            min_height: -0.05
            max_height: 0.10

            <!-- angle_min: -180.9
            angle_max: 5   -->
            angle_increment: 0.05
            scan_time: 0.10
            range_min: 0.1
            range_max: 10.0
            use_inf: true

            # Concurrency level, affects number of pointclouds queued for processing and number of threads used
            # 0 : Detect number of cores
            # 1 : Single threaded
            # 2->inf : Parallelism level
            concurrency_level: 16
        </rosparam>
        <!-- <rosparam file="$(find unitree_move_base)/config/pointCloud_to_laserScan_params.yaml" command="load" /> -->
    </node>
</launch>